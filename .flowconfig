[ignore]

.*/build/.*
.*/test/.*
.*/config/.*
.*/static/.*
.*/node_modules/**/.*

[include]


[libs]
./src/**.*
./src/interfaces.js
./flow-typed/
flow/
node_modules/vue-flow-definitions/definitions/

[lints]

[options]
module.system=haste
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.decorators=ignore

module.file_ext=.vue
module.file_ext=.js
# webpack module alias
module.name_mapper='^src/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'

[strict]

[untyped]